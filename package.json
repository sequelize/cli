{
  "name": "sequelize-cli",
  "version": "2.7.0",
  "description": "The Sequelize CLI",
  "bin": {
    "sequelize": "./bin/sequelize"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "cli-color": "^1.2.0",
    "findup-sync": "^1.0.0",
    "fs-extra": "^4.0.1",
    "js-beautify": "^1.6.14",
    "lodash": "^4.17.4",
    "umzug": "^2.0.1",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015-node4": "^2.1.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^4.4.0",
    "mocha": "^3.3.0",
    "mysql": "^2.13.0",
    "pg": "^6.1.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.0.0",
    "sqlite3": "^3.1.8",
    "unexpected": "^10.32.1"
  },
  "scripts": {
    "build": "babel src -d lib && npm run build:bin && npm run build:assets",
    "build:bin": "babel ./sequelize.js --out-file ./bin/sequelize && chmod +x ./bin/sequelize",
    "build:assets": "cp -R ./src/assets ./lib/",
    "test": "mocha --compilers js:babel-register test"
  },
